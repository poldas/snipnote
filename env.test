APP_ENV=test
APP_DEBUG=1
APP_SECRET=test-app-secret

# Database connection used by PHPUnit (Doctrine suffix will append "_test")
DATABASE_URL="postgresql://app:app@database:5432/app?serverVersion=16&charset=utf8"

# JWT secret for HS256 signing in tests
JWT_SECRET=test-jwt-secret

# Disable outbound email in tests
MAILER_DSN=null://null

# Refresh token TTL for tests (seconds)
REFRESH_TOKEN_TTL_SECONDS=1200

